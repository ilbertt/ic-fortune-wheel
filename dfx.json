{
  "canisters": {
    "backend": {
      "candid": "src/backend/api/backend.did",
      "package": "backend_impl",
      "type": "rust",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "./src/declarations/backend"
      }
    },
    "frontend": {
      "dependencies": ["backend"],
      "source": ["src/frontend/out"],
      "type": "assets",
      "build": ["pnpm build"]
    },
    "internet-identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "exchange-rate": {
      "type": "pull",
      "id": "uf6dk-hyaaa-aaaaq-qaaaq-cai"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "replica": {
        "subnet_type": "system"
      }
    },
    "staging": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  }
}
