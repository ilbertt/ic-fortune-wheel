{
  "canisters": {
    "backend": {
      "type": "custom",
      "gzip": true,
      "optimize": "cycles",
      "wasm": "./target/wasm32-unknown-unknown/release/backend_impl.wasm",
      "build": [
        "pnpm build",
        "cargo build --target wasm32-unknown-unknown --release -p backend_impl --locked"
      ],
      "candid": "src/backend/api/backend.did",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "./src/declarations/backend"
      }
    },
    "internet-identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "exchange-rate": {
      "type": "pull",
      "id": "uf6dk-hyaaa-aaaaq-qaaaq-cai"
    },
    "icp-ledger": {
      "type": "pull",
      "id": "ryjl3-tyaaa-aaaaa-aaaba-cai"
    },
    "ckbtc-ledger": {
      "type": "pull",
      "id": "mxzaz-hqaaa-aaaar-qaada-cai"
    },
    "cketh-ledger": {
      "type": "pull",
      "id": "ss2fx-dyaaa-aaaar-qacoq-cai"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "replica": {
        "subnet_type": "system"
      }
    },
    "staging": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    }
  }
}
